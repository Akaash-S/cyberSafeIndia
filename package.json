{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:extension": "npm run build:extension:clean && npm run build:extension:bundle && npm run build:extension:copy",
    "build:extension:clean": "rimraf dist-extension",
    "build:extension:bundle": "tsc -b && vite build --config vite.extension.config.ts",
    "build:extension:copy": "cp -r public/icons dist-extension/ && cp src/extension/manifest.json dist-extension/",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "@types/qrcode": "^1.5.5",
    "@types/xlsx": "^0.0.35",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "firebase": "^12.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-firebase-hooks": "^5.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chrome": "^0.1.6",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
